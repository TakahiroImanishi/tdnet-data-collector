  constructor(message) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().f[2]++;
    cov_1tjtfrdu5w().s[47]++;
    super(message);
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[48]++;
    this.name = 'UnauthorizedError';
  }
}
/**
 * APIキー認証の検証
 *
 * @param event API Gateway Proxy Event
 * @throws UnauthorizedError APIキーが無効な場合
 */
async function validateApiKey(event) {
  /* istanbul ignore next */
  cov_1tjtfrdu5w().f[3]++;
  const apiKey =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[49]++, 
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[10][0]++, event.headers['x-api-key']) ||
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[10][1]++, event.headers['X-Api-Key']));
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[50]++;
  if (!apiKey) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[11][0]++;
    cov_1tjtfrdu5w().s[51]++;
    throw new UnauthorizedError('API key is required. Please provide x-api-key header.');
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[11][1]++;
  }
  // Secrets ManagerからAPIキーを取得
  const validApiKey =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[52]++, await getApiKey());
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[53]++;
  if (apiKey !== validApiKey) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[12][0]++;
    cov_1tjtfrdu5w().s[54]++;
    throw new UnauthorizedError('Invalid API key');
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[12][1]++;
  }
}
/**
 * クエリパラメータのパース
