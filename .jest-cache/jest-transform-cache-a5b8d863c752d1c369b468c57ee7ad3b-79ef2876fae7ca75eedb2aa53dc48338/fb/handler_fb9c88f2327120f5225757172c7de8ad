 * @param event API Gateway Proxy Event
 * @returns パース済みクエリパラメータ
 * @throws ValidationError バリデーションエラー
 */
function parseQueryParameters(event) {
  /* istanbul ignore next */
  cov_1tjtfrdu5w().f[4]++;
  const params =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[55]++, 
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[13][0]++, event.queryStringParameters) ||
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[13][1]++, {}));
  // フォーマットのバリデーション
  const format =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[56]++, 
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[14][0]++, params.format) ||
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[14][1]++, 'json'));
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[57]++;
  if (!['json', 'csv'].includes(format)) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[15][0]++;
    cov_1tjtfrdu5w().s[58]++;
    throw new errors_1.ValidationError(`Invalid format: ${format}. Expected 'json' or 'csv'.`);
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[15][1]++;
  }
  // リミットのバリデーション（デフォルト: 100、最大: 1000）
  let limit =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[59]++, 100);
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[60]++;
  if (params.limit) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[16][0]++;
    cov_1tjtfrdu5w().s[61]++;
    limit = parseInt(params.limit, 10);
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[62]++;
    if (
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().b[18][0]++, isNaN(limit)) ||
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().b[18][1]++, limit < 1) ||
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().b[18][2]++, limit > 1000)) {
      /* istanbul ignore next */
      cov_1tjtfrdu5w().b[17][0]++;
      cov_1tjtfrdu5w().s[63]++;
      throw new errors_1.ValidationError(`Invalid limit: ${params.limit}. Expected a number between 1 and 1000.`);
    } else
    /* istanbul ignore next */
    {
      cov_1tjtfrdu5w().b[17][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[16][1]++;
  }
  // オフセットのバリデーション（デフォルト: 0）
  let offset =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[64]++, 0);
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[65]++;
  if (params.offset) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[19][0]++;
    cov_1tjtfrdu5w().s[66]++;
    offset = parseInt(params.offset, 10);
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[67]++;
    if (
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().b[21][0]++, isNaN(offset)) ||
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().b[21][1]++, offset < 0)) {
      /* istanbul ignore next */
      cov_1tjtfrdu5w().b[20][0]++;
      cov_1tjtfrdu5w().s[68]++;
      throw new errors_1.ValidationError(`Invalid offset: ${params.offset}. Expected a non-negative number.`);
    } else
    /* istanbul ignore next */
    {
      cov_1tjtfrdu5w().b[20][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[19][1]++;
  }
  // 日付フォーマットのバリデーション
  cov_1tjtfrdu5w().s[69]++;
  if (params.start_date) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[22][0]++;
    cov_1tjtfrdu5w().s[70]++;
    validateDateFormat(params.start_date, 'start_date');
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[22][1]++;
  }
  cov_1tjtfrdu5w().s[71]++;
  if (params.end_date) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[23][0]++;
    cov_1tjtfrdu5w().s[72]++;
    validateDateFormat(params.end_date, 'end_date');
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[23][1]++;
  }
  // 日付範囲の順序性チェック（Property 8）
  cov_1tjtfrdu5w().s[73]++;
  if (
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[25][0]++, params.start_date) &&
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[25][1]++, params.end_date)) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[24][0]++;
    const startDate =
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().s[74]++, new Date(params.start_date));
    const endDate =
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().s[75]++, new Date(params.end_date));
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[76]++;
    if (startDate > endDate) {
      /* istanbul ignore next */
      cov_1tjtfrdu5w().b[26][0]++;
      cov_1tjtfrdu5w().s[77]++;
      throw new errors_1.ValidationError(`start_date (${params.start_date}) must be before or equal to end_date (${params.end_date})`);
    } else
    /* istanbul ignore next */
    {
      cov_1tjtfrdu5w().b[26][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[24][1]++;
  }
  // 企業コードのバリデーション（4桁の数字）
  cov_1tjtfrdu5w().s[78]++;
  if (params.company_code) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[27][0]++;
    const companyCodeRegex =
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().s[79]++, /^\d{4}$/);
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[80]++;
    if (!companyCodeRegex.test(params.company_code)) {
      /* istanbul ignore next */
      cov_1tjtfrdu5w().b[28][0]++;
      cov_1tjtfrdu5w().s[81]++;
