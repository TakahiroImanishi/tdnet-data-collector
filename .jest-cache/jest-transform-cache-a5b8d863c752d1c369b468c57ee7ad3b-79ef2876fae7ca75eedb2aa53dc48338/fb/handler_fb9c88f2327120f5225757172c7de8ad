    } else
    /* istanbul ignore next */
    {
      cov_1tjtfrdu5w().b[28][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[27][1]++;
  }
  cov_1tjtfrdu5w().s[82]++;
  return {
    company_code: params.company_code,
    start_date: params.start_date,
    end_date: params.end_date,
    disclosure_type: params.disclosure_type,
    format,
    limit,
    offset
  };
}
/**
 * 日付フォーマットのバリデーション
 *
 * @param date 日付文字列（YYYY-MM-DD）
 * @param fieldName フィールド名
 * @throws ValidationError バリデーションエラー
 */
function validateDateFormat(date, fieldName) {
  /* istanbul ignore next */
  cov_1tjtfrdu5w().f[5]++;
  // YYYY-MM-DD形式のチェック
  const dateRegex =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[83]++, /^\d{4}-\d{2}-\d{2}$/);
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[84]++;
  if (!dateRegex.test(date)) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[29][0]++;
    cov_1tjtfrdu5w().s[85]++;
    throw new errors_1.ValidationError(`Invalid ${fieldName} format: ${date}. Expected YYYY-MM-DD format.`);
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[29][1]++;
  }
  // 有効な日付かチェック
  const parsedDate =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[86]++, new Date(date));
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[87]++;
  if (isNaN(parsedDate.getTime())) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[30][0]++;
    cov_1tjtfrdu5w().s[88]++;
    throw new errors_1.ValidationError(`Invalid ${fieldName}: ${date}. Date does not exist.`);
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[30][1]++;
  }
  // 日付の整合性チェック（例: 2023-02-29は2023-03-01に変換されるため検出）
  const [year, month, day] =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[89]++, date.split('-').map(Number));
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[90]++;
  if (
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[32][0]++, parsedDate.getFullYear() !== year) ||
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[32][1]++, parsedDate.getMonth() + 1 !== month) ||
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[32][2]++, parsedDate.getDate() !== day)) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[31][0]++;
    cov_1tjtfrdu5w().s[91]++;
    throw new errors_1.ValidationError(`Invalid ${fieldName}: ${date}. Date does not exist.`);
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[31][1]++;
  }
}
/**
 * エラーハンドリング
 *
 * カスタムエラーを適切なHTTPステータスコードとエラーコードに変換します。
 * API設計ガイドラインに準拠した形式でエラーレスポンスを返します。
 *
 * @param error エラーオブジェクト
 * @param requestId リクエストID
 * @returns API Gateway Proxy Result
 */
function handleError(error, requestId) {
  /* istanbul ignore next */
  cov_1tjtfrdu5w().f[6]++;
  // エラー種別に応じたステータスコードとエラーコード
  let statusCode =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[92]++, 500);
  let errorCode =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[93]++, 'INTERNAL_ERROR');
  let details =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[94]++, {});
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[95]++;
  if (error instanceof errors_1.ValidationError) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[33][0]++;
    cov_1tjtfrdu5w().s[96]++;
    statusCode = 400;
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[97]++;
    errorCode = 'VALIDATION_ERROR';
    // ValidationErrorの詳細情報を含める
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[98]++;
    if (error.details) {
      /* istanbul ignore next */
      cov_1tjtfrdu5w().b[34][0]++;
      cov_1tjtfrdu5w().s[99]++;
      details = error.details;
    } else
    /* istanbul ignore next */
    {
      cov_1tjtfrdu5w().b[34][1]++;
    }
  } else {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[33][1]++;
    cov_1tjtfrdu5w().s[100]++;
    if (error instanceof errors_1.NotFoundError) {
      /* istanbul ignore next */
      cov_1tjtfrdu5w().b[35][0]++;
      cov_1tjtfrdu5w().s[101]++;
      statusCode = 404;
      /* istanbul ignore next */
      cov_1tjtfrdu5w().s[102]++;
      errorCode = 'NOT_FOUND';
    } else {
      /* istanbul ignore next */
      cov_1tjtfrdu5w().b[35][1]++;
      cov_1tjtfrdu5w().s[103]++;
      if (error instanceof UnauthorizedError) {
        /* istanbul ignore next */
        cov_1tjtfrdu5w().b[36][0]++;
        cov_1tjtfrdu5w().s[104]++;
        statusCode = 401;
        /* istanbul ignore next */
        cov_1tjtfrdu5w().s[105]++;
        errorCode = 'UNAUTHORIZED';
      } else {
        /* istanbul ignore next */
        cov_1tjtfrdu5w().b[36][1]++;
        cov_1tjtfrdu5w().s[106]++;
        if (error.message.includes('API key')) {
          /* istanbul ignore next */
          cov_1tjtfrdu5w().b[37][0]++;
          cov_1tjtfrdu5w().s[107]++;
          statusCode = 401;
          /* istanbul ignore next */
          cov_1tjtfrdu5w().s[108]++;
          errorCode = 'UNAUTHORIZED';
        } else
        /* istanbul ignore next */
        {
          cov_1tjtfrdu5w().b[37][1]++;
        }
      }
    }
  }
  // API設計ガイドラインに準拠したエラーレスポンス形式
  const errorResponse =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[109]++, {
    status: 'error',
    error: {
      code: errorCode,
      message: error.message,
      details
    },
    request_id: requestId
  });
  // 本番環境ではスタックトレースを含めない
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[110]++;
  if (process.env.NODE_ENV !== 'production') {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[38][0]++;
    cov_1tjtfrdu5w().s[111]++;
    errorResponse.error.stack = error.stack;
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[38][1]++;
  }
  cov_1tjtfrdu5w().s[112]++;
  return {
    statusCode,
    headers: {
      'Content-Type': 'application/json',
      'Access-Control-Allow-Origin': '*'
    },
    body: JSON.stringify(errorResponse)
  };
}
