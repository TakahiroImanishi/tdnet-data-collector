/* istanbul ignore next */
(cov_1tjtfrdu5w().s[8]++, new client_secrets_manager_1.SecretsManagerClient({
  region: 
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[0][0]++, process.env.AWS_REGION) ||
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[0][1]++, 'ap-northeast-1')
}));
// APIキーキャッシュ（5分TTL）
let cachedApiKey =
/* istanbul ignore next */
(cov_1tjtfrdu5w().s[9]++, null);
let cacheExpiry =
/* istanbul ignore next */
(cov_1tjtfrdu5w().s[10]++, 0);
/**
 * Secrets ManagerからAPIキーを取得
 *
 * テスト環境（TEST_ENV=e2e）では、API_KEY環境変数から直接取得します。
 * 本番環境では、Secrets Managerから取得します。
 *
 * @returns APIキー
 * @throws Error Secrets Managerからの取得に失敗した場合
 */
async function getApiKey() {
  /* istanbul ignore next */
  cov_1tjtfrdu5w().f[0]++;
  cov_1tjtfrdu5w().s[11]++;
  // キャッシュチェック
  if (
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[2][0]++, cachedApiKey) &&
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[2][1]++, Date.now() < cacheExpiry)) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[1][0]++;
    cov_1tjtfrdu5w().s[12]++;
    return cachedApiKey;
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[1][1]++;
  }
  // テスト環境: API_KEY環境変数から直接取得
  cov_1tjtfrdu5w().s[13]++;
  if (
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[4][0]++, process.env.TEST_ENV === 'e2e') &&
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().b[4][1]++, process.env.API_KEY)) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[3][0]++;
    cov_1tjtfrdu5w().s[14]++;
    cachedApiKey = process.env.API_KEY;
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[15]++;
    cacheExpiry = Date.now() + 5 * 60 * 1000;
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[16]++;
    return cachedApiKey;
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[3][1]++;
  }
  // 本番環境: Secrets Managerから取得
  const secretArn =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[17]++, process.env.API_KEY_SECRET_ARN);
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[18]++;
  if (!secretArn) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().b[5][0]++;
    cov_1tjtfrdu5w().s[19]++;
    throw new Error('API_KEY_SECRET_ARN environment variable is not set');
  } else
  /* istanbul ignore next */
  {
    cov_1tjtfrdu5w().b[5][1]++;
  }
  cov_1tjtfrdu5w().s[20]++;
  try {
    const command =
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().s[21]++, new client_secrets_manager_1.GetSecretValueCommand({
      SecretId: secretArn
    }));
    const response =
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().s[22]++, await secretsClient.send(command));
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[23]++;
    if (!response.SecretString) {
      /* istanbul ignore next */
      cov_1tjtfrdu5w().b[6][0]++;
      cov_1tjtfrdu5w().s[24]++;
      throw new Error('Secret value is empty');
    } else
    /* istanbul ignore next */
    {
      cov_1tjtfrdu5w().b[6][1]++;
    }
    // APIキーをキャッシュ（5分TTL）
    cov_1tjtfrdu5w().s[25]++;
    cachedApiKey = response.SecretString;
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[26]++;
    cacheExpiry = Date.now() + 5 * 60 * 1000;
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[27]++;
    return cachedApiKey;
  } catch (error) {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[28]++;
    logger_1.logger.error('Failed to retrieve API key from Secrets Manager', {
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_1tjtfrdu5w().b[7][0]++, error.message) :
      /* istanbul ignore next */
      (cov_1tjtfrdu5w().b[7][1]++, String(error)),
      secret_arn: secretArn
    });
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[29]++;
    throw new Error('Failed to retrieve API key');
  }
}
/**
 * Lambda Queryハンドラー
 *
 * @param event API Gateway Proxy Event
 * @param context Lambda Context
 * @returns API Gateway Proxy Result
 *
 * @example
 * ```typescript
 * // 企業コードで検索
 * GET /disclosures?company_code=7203
 *
 * // 日付範囲で検索
 * GET /disclosures?start_date=2024-01-15&end_date=2024-01-20
 *
 * // CSV形式で取得
 * GET /disclosures?format=csv&start_date=2024-01-15&end_date=2024-01-20
 * ```
 */
async function handler(event, context) {
  /* istanbul ignore next */
  cov_1tjtfrdu5w().f[1]++;
  const startTime =
  /* istanbul ignore next */
  (cov_1tjtfrdu5w().s[30]++, Date.now());
  /* istanbul ignore next */
  cov_1tjtfrdu5w().s[31]++;
  try {
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[32]++;
    logger_1.logger.info('Lambda Query started', {
      queryStringParameters: event.queryStringParameters,
      request_id: context.awsRequestId,
      function_name: context.functionName
    });
    // APIキー認証の検証
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[33]++;
    await validateApiKey(event);
    // クエリパラメータのパース
    const params =
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().s[34]++, parseQueryParameters(event));
    // 開示情報を検索
    const result =
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().s[35]++, await (0, query_disclosures_1.queryDisclosures)(params));
    const duration =
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().s[36]++, Date.now() - startTime);
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[37]++;
    logger_1.logger.info('Lambda Query completed', {
      request_id: context.awsRequestId,
      count: result.count,
      duration_ms: duration
    });
    // 成功メトリクス送信
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[38]++;
    await (0, cloudwatch_metrics_1.sendMetrics)([{
      name: 'LambdaExecutionTime',
      value: duration,
      unit: 'Milliseconds',
      dimensions: {
        FunctionName: 'Query'
      }
    }, {
      name: 'QueryResultCount',
      value: result.count,
      unit: 'Count',
      dimensions: {
        Format: params.format
      }
    }]);
    // フォーマット別レスポンス
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[39]++;
    if (params.format === 'csv') {
      /* istanbul ignore next */
      cov_1tjtfrdu5w().b[8][0]++;
      const csv =
      /* istanbul ignore next */
      (cov_1tjtfrdu5w().s[40]++, (0, format_csv_1.formatAsCsv)(result.disclosures));
      /* istanbul ignore next */
      cov_1tjtfrdu5w().s[41]++;
      return {
        statusCode: 200,
        headers: {
          'Content-Type': 'text/csv',
          'Content-Disposition': `attachment; filename="disclosures-${Date.now()}.csv"`,
          'Access-Control-Allow-Origin': '*'
        },
        body: csv
      };
    } else
    /* istanbul ignore next */
    {
      cov_1tjtfrdu5w().b[8][1]++;
    }
    // JSON形式（デフォルト）
    cov_1tjtfrdu5w().s[42]++;
    return {
      statusCode: 200,
      headers: {
        'Content-Type': 'application/json',
        'Access-Control-Allow-Origin': '*'
      },
      body: JSON.stringify(result)
    };
  } catch (error) {
    const duration =
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().s[43]++, Date.now() - startTime);
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[44]++;
    logger_1.logger.error('Lambda Query failed', (0, logger_1.createErrorContext)(error, {
      request_id: context.awsRequestId,
      duration_ms: duration
    }));
    // エラーメトリクス送信
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[45]++;
    await (0, cloudwatch_metrics_1.sendErrorMetric)(error instanceof Error ?
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().b[9][0]++, error.constructor.name) :
    /* istanbul ignore next */
    (cov_1tjtfrdu5w().b[9][1]++, 'Unknown'), 'Query');
    /* istanbul ignore next */
    cov_1tjtfrdu5w().s[46]++;
    return handleError(error, context.awsRequestId);
  }
}
