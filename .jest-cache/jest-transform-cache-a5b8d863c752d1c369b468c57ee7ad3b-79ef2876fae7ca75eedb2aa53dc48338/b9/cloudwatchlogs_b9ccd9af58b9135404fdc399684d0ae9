      case logs.RetentionDays.THREE_MONTHS:
        /* istanbul ignore next */
        cov_23riszpk42().b[19][1]++;
        cov_23riszpk42().s[53]++;
        return 90;
      default:
        /* istanbul ignore next */
        cov_23riszpk42().b[19][2]++;
        cov_23riszpk42().s[54]++;
