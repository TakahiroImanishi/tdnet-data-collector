    // 環境ごとのログ保持期間を設定
    /* istanbul ignore next */
    cov_23riszpk42().s[41]++;
    if (props.environment === 'prod') {
      /* istanbul ignore next */
      cov_23riszpk42().b[17][0]++;
      cov_23riszpk42().s[42]++;
      // 本番環境: 3ヶ月（90日）
      this.retentionDays = logs.RetentionDays.THREE_MONTHS;
      // 本番環境: スタック削除時もログを保持
      /* istanbul ignore next */
      cov_23riszpk42().s[43]++;
      this.removalPolicy = cdk.RemovalPolicy.RETAIN;
    } else {
      /* istanbul ignore next */
      cov_23riszpk42().b[17][1]++;
      cov_23riszpk42().s[44]++;
      // 開発環境: 1週間（7日）
