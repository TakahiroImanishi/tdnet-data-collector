        it('有効なリクエストで200を返す', async () => {
            // Lambda Collectorの呼び出しをモック
            lambdaMock.on(client_lambda_1.InvokeCommand).resolves({
                StatusCode: 202,
            });
            const event = {
                body: JSON.stringify({
                    start_date: '2024-01-15',
                    end_date: '2024-01-20',
