      } else
      /* istanbul ignore next */
      {
        cov_kpri68apt().b[4][1]++;
      }
      // 遅延時間を計算（指数バックオフ）
      const delay =
      /* istanbul ignore next */
      (cov_kpri68apt().s[17]++, calculateDelay(attempt, opts));
      // 遅延後に再試行
      /* istanbul ignore next */
      cov_kpri68apt().s[18]++;
      await sleep(delay);
      /* istanbul ignore next */
      cov_kpri68apt().s[19]++;
      attempt++;
    }
  }
  // この行には到達しないはずだが、TypeScriptの型チェックのために必要
  /* istanbul ignore next */
  cov_kpri68apt().s[20]++;
  throw /* istanbul ignore next */(cov_kpri68apt().b[5][0]++, lastError) ||
  /* istanbul ignore next */
  (cov_kpri68apt().b[5][1]++, new Error('Unexpected error in retryWithBackoff'));
}
/**
 * 遅延時間を計算（指数バックオフ + ジッター）
 *
 * @param attempt 現在の試行回数（0から始まる）
 * @param options 再試行オプション
 * @returns 遅延時間（ミリ秒）
 */
function calculateDelay(attempt, options) {
  /* istanbul ignore next */
  cov_kpri68apt().f[2]++;
  // 指数バックオフ: initialDelay * (backoffMultiplier ^ attempt)
  const exponentialDelay =
  /* istanbul ignore next */
  (cov_kpri68apt().s[21]++, options.initialDelay * Math.pow(options.backoffMultiplier, attempt));
  // ジッターを追加（0〜exponentialDelayのランダム値）
  /* istanbul ignore next */
  cov_kpri68apt().s[22]++;
  if (options.jitter) {
    /* istanbul ignore next */
    cov_kpri68apt().b[6][0]++;
    cov_kpri68apt().s[23]++;
    return Math.random() * exponentialDelay;
  } else
  /* istanbul ignore next */
  {
    cov_kpri68apt().b[6][1]++;
  }
  cov_kpri68apt().s[24]++;
  return exponentialDelay;
}
/**
 * 指定時間スリープ
 *
 * @param ms スリープ時間（ミリ秒）
 */
function sleep(ms) {
  /* istanbul ignore next */
  cov_kpri68apt().f[3]++;
  cov_kpri68apt().s[25]++;
  return new Promise(resolve => {
    /* istanbul ignore next */
    cov_kpri68apt().f[4]++;
    cov_kpri68apt().s[26]++;
    return setTimeout(resolve, ms);
  });
}
/**
 * 再試行可能なエラーかどうかを判定するヘルパー関数
 *
 * @param error エラーオブジェクト
 * @returns 再試行可能な場合はtrue
 *
 * @example
 * ```typescript
 * try {
 *   await operation();
 * } catch (error) {
 *   if (isRetryableError(error)) {
 *     // 再試行ロジック
 *   } else {
 *     // 即座に失敗
 *   }
 * }
 * ```
 */
function isRetryableError(error) {
  /* istanbul ignore next */
  cov_kpri68apt().f[5]++;
  cov_kpri68apt().s[27]++;
  if (!(error instanceof Error)) {
    /* istanbul ignore next */
    cov_kpri68apt().b[7][0]++;
    cov_kpri68apt().s[28]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_kpri68apt().b[7][1]++;
  }
  // RetryableErrorまたはそのサブクラス
  cov_kpri68apt().s[29]++;
  if (error instanceof errors_1.RetryableError) {
    /* istanbul ignore next */
    cov_kpri68apt().b[8][0]++;
    cov_kpri68apt().s[30]++;
    return true;
  } else
  /* istanbul ignore next */
  {
    cov_kpri68apt().b[8][1]++;
  }
  // ネットワークエラー
  const networkErrors =
  /* istanbul ignore next */
  (cov_kpri68apt().s[31]++, ['ECONNRESET', 'ETIMEDOUT', 'ENOTFOUND', 'ECONNREFUSED']);
  /* istanbul ignore next */
  cov_kpri68apt().s[32]++;
  if (networkErrors.some(code => {
    /* istanbul ignore next */
    cov_kpri68apt().f[6]++;
    cov_kpri68apt().s[33]++;
    return error.message.includes(code);
  })) {
    /* istanbul ignore next */
    cov_kpri68apt().b[9][0]++;
    cov_kpri68apt().s[34]++;
    return true;
  } else
  /* istanbul ignore next */
  {
    cov_kpri68apt().b[9][1]++;
  }
  // HTTPタイムアウト
  cov_kpri68apt().s[35]++;
  if (error.message.includes('timeout')) {
    /* istanbul ignore next */
    cov_kpri68apt().b[10][0]++;
    cov_kpri68apt().s[36]++;
    return true;
  } else
  /* istanbul ignore next */
  {
    cov_kpri68apt().b[10][1]++;
  }
  // AWS一時的エラー
  const awsErrors =
  /* istanbul ignore next */
  (cov_kpri68apt().s[37]++, ['ThrottlingException', 'ServiceUnavailable', 'RequestTimeout']);
  /* istanbul ignore next */
  cov_kpri68apt().s[38]++;
  if (awsErrors.some(code => {
    /* istanbul ignore next */
    cov_kpri68apt().f[7]++;
    cov_kpri68apt().s[39]++;
    return error.message.includes(code);
  })) {
    /* istanbul ignore next */
    cov_kpri68apt().b[11][0]++;
    cov_kpri68apt().s[40]++;
    return true;
  } else
  /* istanbul ignore next */
  {
    cov_kpri68apt().b[11][1]++;
  }
  cov_kpri68apt().s[41]++;
  return false;
}
