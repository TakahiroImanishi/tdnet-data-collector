            const invalid = { ...validDisclosure, collected_at: '2024/01/15 10:35:00' };
            expect(() => (0, disclosure_1.validateDisclosure)(invalid)).toThrow(errors_1.ValidationError);
        });
        it('company_codeが4桁の数字でない場合はValidationErrorをスロー', () => {
            const invalid1 = { ...validDisclosure, company_code: '123' }; // 3桁
            expect(() => (0, disclosure_1.validateDisclosure)(invalid1)).toThrow(errors_1.ValidationError);
            expect(() => (0, disclosure_1.validateDisclosure)(invalid1)).toThrow(/Invalid company_code format/);
            const invalid2 = { ...validDisclosure, company_code: '12345' }; // 5桁
            expect(() => (0, disclosure_1.validateDisclosure)(invalid2)).toThrow(errors_1.ValidationError);
            const invalid3 = { ...validDisclosure, company_code: 'ABCD' }; // 英字
            expect(() => (0, disclosure_1.validateDisclosure)(invalid3)).toThrow(errors_1.ValidationError);
        });
        it('date_partitionがYYYY-MM形式でない場合はValidationErrorをスロー', () => {
            const invalid1 = { ...validDisclosure, date_partition: '2024-1' }; // 月が1桁
            expect(() => (0, disclosure_1.validateDisclosure)(invalid1)).toThrow(errors_1.ValidationError);
            expect(() => (0, disclosure_1.validateDisclosure)(invalid1)).toThrow(/Invalid date_partition format/);
            const invalid2 = { ...validDisclosure, date_partition: '202401' }; // ハイフンなし
            expect(() => (0, disclosure_1.validateDisclosure)(invalid2)).toThrow(errors_1.ValidationError);
            const invalid3 = { ...validDisclosure, date_partition: '2024/01' }; // スラッシュ
            expect(() => (0, disclosure_1.validateDisclosure)(invalid3)).toThrow(errors_1.ValidationError);
