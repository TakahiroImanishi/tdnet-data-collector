      period: cdk.Duration.minutes(5)
    }));
    const invocations =
    /* istanbul ignore next */
    (cov_1njnsr0wer().s[75]++, lambdaFunction.metricInvocations({
      statistic: 'Sum',
      period: cdk.Duration.minutes(5)
    }));
    // Error Rate = (Errors / Invocations) * 100
    /* istanbul ignore next */
    cov_1njnsr0wer().s[76]++;
    return new cloudwatch.MathExpression({
      expression: '(errors / invocations) * 100',
      usingMetrics: {
        errors,
        invocations
      },
      label: 'Error Rate (%)',
      period: cdk.Duration.minutes(5)
    });
  }
  /**
   * CollectionSuccessRate
   *
   * Success Rate = (DisclosuresCollected / (DisclosuresCollected + DisclosuresFailed)) * 100
   */
  createCollectionSuccessRateMetric(environment) {
    /* istanbul ignore next */
    cov_1njnsr0wer().f[12]++;
    const collected =
    /* istanbul ignore next */
    (cov_1njnsr0wer().s[77]++, new cloudwatch.Metric({
      namespace: 'TDnet/Collector',
      metricName: 'DisclosuresCollected',
      statistic: 'Sum',
      period: cdk.Duration.hours(1),
      dimensionsMap: {
        Environment: environment
      }
    }));
    const failed =
    /* istanbul ignore next */
    (cov_1njnsr0wer().s[78]++, new cloudwatch.Metric({
      namespace: 'TDnet/Collector',
      metricName: 'DisclosuresFailed',
      statistic: 'Sum',
      period: cdk.Duration.hours(1),
      dimensionsMap: {
        Environment: environment
      }
    }));
    // Success Rate = (Collected / (Collected + Failed)) * 100
    /* istanbul ignore next */
    cov_1njnsr0wer().s[79]++;
    return new cloudwatch.MathExpression({
      expression: '(collected / (collected + failed)) * 100',
      usingMetrics: {
        collected,
        failed
      },
      label: 'Collection Success Rate (%)',
      period: cdk.Duration.hours(1)
    });
  }
}
