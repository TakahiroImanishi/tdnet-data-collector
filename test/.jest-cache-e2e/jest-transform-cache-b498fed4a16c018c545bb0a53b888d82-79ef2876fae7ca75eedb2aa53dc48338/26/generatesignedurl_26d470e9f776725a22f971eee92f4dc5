        logger_1.logger.info('Generating signed URL', {
            s3_key,
            expires_in: expiresIn,
        });
        // GetObjectCommandを作成
        const command = new client_s3_1.GetObjectCommand({
            Bucket: EXPORT_BUCKET,
            Key: s3_key,
        });
